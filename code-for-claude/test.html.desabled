{/* Toggle Button */}
      <div id="toggle-btn" className="olm-btn">
        <div className="olm-btn-outline olm-btn-outline-1"></div>
        <div className="olm-btn-outline olm-btn-outline-2"></div>
        
      </div>

      .olm-btn-outline-1 {
  border-radius: 60% 40% 30% 70% / 60% 30% 70% 40%;
  animation: olm-morph 4s linear infinite;
}

.olm-btn-outline-2 {
  border-radius: 53% 47% 43% 58% / 51% 39% 61% 49%;
}

@keyframes olm-morph {
  0% {
    border-radius: 60% 40% 30% 70% / 60% 30% 70% 40%;
  }
  50% {
    border-radius: 30% 60% 70% 40% / 50% 60% 30% 60%;
  }
  100% {
    border-radius: 60% 40% 30% 70% / 60% 30% 70% 40%;
  }
}